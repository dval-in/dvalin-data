{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "_id": {
      "type": "number"
    },
    "achievements": {
      "items": {
        "properties": {
          "desc": {
            "type": "string"
          },
          "hidden": {
            "type": "boolean"
          },
          "id": {
            "type": "number"
          },
          "name": {
            "type": "string"
          },
          "order": {
            "type": "number"
          },
          "quest": {
            "type": "string"
          },
          "questType": {
            "enum": [
              "archonQuests",
              "commissions",
              "worldQuests"
            ],
            "type": "string"
          },
          "region": {
            "type": "string"
          },
          "reward": {
            "type": "number"
          },
          "version": {
            "type": "string"
          }
        },
        "required": [
          "desc",
          "hidden",
          "id",
          "name",
          "order",
          "reward",
          "version"
        ],
        "type": "object"
      },
      "type": "array"
    },
    "id": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "order": {
      "type": "number"
    }
  },
  "required": [
    "_id",
    "achievements",
    "id",
    "name",
    "order"
  ],
  "type": "object",
  "additionalProperties": false
}