{
  "$defs": {
    "EventLocalized": {
      "properties": {
        "postId": {
          "title": "Postid",
          "type": "string"
        },
        "gameId": {
          "enum": [
            "HonkaiImpact3Rd",
            "GenshinImpact",
            "TearsOfThemis",
            "Hoyolab",
            "HonkaiStarRail",
            "ZenlessZoneZero"
          ]
        },
        "messageType": {
          "enum": [
            "Notices",
            "Event",
            "Info"
          ]
        },
        "createdAt": {
          "title": "Createdat",
          "type": "string"
        },
        "tags": {
          "items": {
            "enum": [
              "NewCharacterIntro",
              "VoiceArtistAnnouncement",
              "VersionEventWishesNotice",
              "VersionEventNoticesCompilation",
              "CharacterDemo",
              "CharacterTeaser",
              "StoryQuest",
              "VersionNewWeapon",
              "NewOutfit",
              "OutfitTeaser",
              "NewContentsDisplay",
              "WebEventWallpapers",
              "VersionPreview",
              "Event",
              "VersionTrailer",
              "OstAlbum",
              "SpecialProgramPreview",
              "DevelopersDiscussion",
              "CollectedMiscellany",
              "VersionEventsPreview",
              "EventTeaser",
              "VersionNewArtifact",
              "UpdatePreview",
              "UpdateDetails",
              "LeyLineOverflow",
              "Wallpapers",
              "GeniusInvokationTcg",
              "CutsceneAnimation",
              "GenshinConcert2023",
              "VersionPreviewPage",
              "Music"
            ]
          },
          "title": "Tags",
          "type": "array"
        },
        "language": {
          "$ref": "#/$defs/LanguageCode"
        },
        "subject": {
          "title": "Subject",
          "type": "string"
        },
        "content": {
          "default": "",
          "title": "Content",
          "type": "string"
        },
        "links": {
          "items": {
            "$ref": "#/$defs/Link"
          },
          "title": "Links",
          "type": "array"
        },
        "articleUrl": {
          "readOnly": true,
          "title": "Articleurl",
          "type": "string"
        }
      },
      "required": [
        "postId",
        "gameId",
        "messageType",
        "createdAt",
        "language",
        "subject",
        "articleUrl"
      ],
      "title": "EventLocalized",
      "type": "object"
    },
    "LanguageCode": {
      "enum": [
        "th-th",
        "zh-cn",
        "zh-tw",
        "ru-ru",
        "id-id",
        "ko-kr",
        "vi-vn",
        "it-it",
        "ja-jp",
        "pt-pt",
        "tr-tr",
        "en-us",
        "de-de",
        "fr-fr",
        "es-es"
      ],
      "title": "LanguageCode",
      "type": "string"
    },
    "Link": {
      "properties": {
        "index": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Index"
        },
        "url": {
          "default": "",
          "title": "Url",
          "type": "string"
        },
        "urlOriginal": {
          "title": "Urloriginal",
          "type": "string"
        },
        "urlOriginalResolved": {
          "$ref": "#/$defs/RedirectLinkChain"
        },
        "urlLocal": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Urllocal"
        },
        "linkType": {
          "default": 12,
          "enum": [
            "Image",
            "HoyoLink",
            "Hoyolab",
            "MihoyoHoyoverse",
            "Twitter",
            "Facebook",
            "Youtube",
            "Twitch",
            "Vk",
            "Telegram",
            "Relative",
            "Unknown",
            "Malformed"
          ]
        }
      },
      "required": [
        "urlOriginal"
      ],
      "title": "Link",
      "type": "object"
    },
    "RedirectLinkChain": {
      "items": {
        "type": "string"
      },
      "title": "RedirectLinkChain",
      "type": "array"
    }
  },
  "description": "A file containing events.",
  "items": {
    "$ref": "#/$defs/EventLocalized"
  },
  "title": "EventFile",
  "type": "array"
}
