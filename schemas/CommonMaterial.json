{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"properties": {
		"craft": {
			"properties": {
				"cost": {
					"type": "number"
				},
				"items": {
					"items": {
						"properties": {
							"amount": {
								"type": "number"
							},
							"id": {
								"type": "string"
							},
							"name": {
								"type": "string"
							}
						},
						"required": ["amount", "id", "name"],
						"type": "object"
					},
					"type": "array"
				}
			},
			"required": ["cost", "items"],
			"type": "object"
		},
		"description": {
			"type": "string"
		},
		"id": {
			"type": "string"
		},
		"name": {
			"type": "string"
		},
		"rarity": {
			"type": "number"
		},
		"source": {
			"items": {
				"type": "string"
			},
			"type": "array"
		},
		"version": {
			"type": "string"
		}
	},
	"required": ["description", "id", "name", "rarity", "source", "version"],
	"type": "object",
	"additionalProperties": false
}
