{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/definitions/Events",
  "definitions": {
    "Events": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/Event"
      }
    },
    "Event": {
      "type": "object",
      "properties": {
        "postId": {
          "type": "string"
        },
        "gameId": {
          "$ref": "#/definitions/Game"
        },
        "messageType": {
          "$ref": "#/definitions/MessageType"
        },
        "createdAt": {
          "type": "string",
          "format": "date-time"
        },
        "tags": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Tag"
          }
        },
        "language": {
          "$ref": "#/definitions/LanguageCode"
        },
        "subject": {
          "type": "string"
        },
        "content": {
          "type": "string"
        },
        "contentOriginal": {
          "type": "string"
        },
        "links": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Link"
          }
        },
        "articleUrl": {
          "type": "string"
        }
      },
      "required": [
        "postId",
        "gameId",
        "messageType",
        "createdAt",
        "tags",
        "language",
        "subject",
        "content",
        "contentOriginal",
        "links",
        "articleUrl"
      ],
      "additionalProperties": false
    },
    "Game": {
      "type": "string",
      "enum": [
        "HonkaiImpact3Rd",
        "GenshinImpact",
        "TearsOfThemis",
        "Hoyolab",
        "HonkaiStarRail",
        "ZenlessZoneZero"
      ]
    },
    "MessageType": {
      "type": "string",
      "enum": [
        "Notices",
        "Event",
        "Info"
      ]
    },
    "Tag": {
      "type": "string",
      "enum": [
        "NewCharacterIntro",
        "VoiceArtistAnnouncement",
        "VersionEventWishesNotice",
        "VersionEventNoticesCompilation",
        "CharacterDemo",
        "CharacterTeaser",
        "StoryQuest",
        "VersionNewWeapon",
        "NewOutfit",
        "OutfitTeaser",
        "NewContentsDisplay",
        "WebEventWallpapers",
        "VersionPreview",
        "Event",
        "VersionTrailer",
        "OstAlbum",
        "SpecialProgramPreview",
        "DevelopersDiscussion",
        "CollectedMiscellany",
        "VersionEventsPreview",
        "EventTeaser",
        "VersionNewArtifact",
        "UpdatePreview",
        "UpdateDetails",
        "LeyLineOverflow",
        "Wallpapers",
        "GeniusInvokationTcg",
        "CutsceneAnimation",
        "GenshinConcert2023",
        "VersionPreviewPage",
        "Music"
      ]
    },
    "LanguageCode": {
      "type": "string",
      "enum": [
        "th-th",
        "zh-cn",
        "zh-tw",
        "ru-ru",
        "id-id",
        "ko-kr",
        "vi-vn",
        "it-it",
        "ja-jp",
        "pt-pt",
        "tr-tr",
        "en-us",
        "de-de",
        "fr-fr",
        "es-es"
      ]
    },
    "Link": {
      "type": "object",
      "properties": {
        "index": {
          "type": "number"
        },
        "url": {
          "type": "string"
        },
        "urlOriginal": {
          "type": "string"
        },
        "urlOriginalResolved": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "urlS3": {
          "type": [
            "string",
            "null"
          ]
        },
        "linkType": {
          "$ref": "#/definitions/LinkType"
        },
        "isResolved": {
          "type": "boolean"
        }
      },
      "required": [
        "index",
        "url",
        "urlOriginal",
        "urlOriginalResolved",
        "linkType",
        "isResolved"
      ],
      "additionalProperties": false
    },
    "LinkType": {
      "type": "string",
      "enum": [
        "Image",
        "HoyoLink",
        "Hoyolab",
        "MihoyoHoyoverse",
        "Twitter",
        "Facebook",
        "Youtube",
        "Twitch",
        "Vk",
        "Telegram",
        "Relative",
        "Unknown",
        "Malformed"
      ]
    }
  }
}